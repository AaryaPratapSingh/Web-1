 @* @model List<Employee> *@
@model Hybrid

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var d in Model.emps)
        {
        <tr>
            <th scope="row">@d.id</th>
            <td>@d.name</td>
            <td>@d.email</td>
            <td>@d.salary</td>
        </tr>
        }
    </tbody>
</table>

