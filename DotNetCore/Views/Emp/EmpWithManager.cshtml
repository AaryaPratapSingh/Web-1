@{
}

@model List<Employee>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Salary</th>
            <th scope="col">Manager</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var d in Model)
        {
            <tr>
                <th scope="row">@d.id</th>
                <td>@d.name</td>
                <td>@d.email</td>
                <td>@d.salary</td>
                @* <td>@d.manager.Mname</td> *@
            </tr>
        }
    </tbody>
</table>
